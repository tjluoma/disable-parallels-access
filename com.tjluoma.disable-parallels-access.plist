<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.tjluoma.disable-parallels-access</string>
	<key>Program</key>
	<string>/usr/local/bin/disable-parallels-access.sh</string>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/disable-parallels-access.log</string>
	<key>StandardOutPath</key>
	<string>/tmp/disable-parallels-access.log</string>
	<key>WatchPaths</key>
	<array>
		<string>/etc/com.parallels.mobile.prl_deskctl_agent.launchd</string>
		<string>/usr/bin/paxctl</string>
		<string>/Library/LaunchDaemons/com.parallels.mobile.dispatcher.launchdaemon.plist</string>
		<string>/Library/LaunchDaemons/com.parallels.mobile.kextloader.launchdaemon.plist</string>
		<string>/Library/LaunchAgents/com.parallels.mobile.prl_deskctl_agent.launchagent.plist</string>
		<string>/Library/LaunchAgents/com.parallels.mobile.startgui.launchagent.plist</string>
	</array>
</dict>
</plist>
